<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "struts-2.1.dtd" >
<struts>
	<package name="car" namespace="/"
		extends="struts-default,json-default">
		<action name="car_*" class="CarAction" method="{1}">
			<result name="carPage">/front/car.jsp</result>
			<result name="addCart">/front/car.jsp</result>
			<result name="clearCart">/front/car.jsp</result>
			<result name="removeCart">/front/car.jsp</result>
			
		</action>
	</package>
</struts>
