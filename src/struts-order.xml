<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "struts-2.1.dtd" >
<struts>
	<package name="order" namespace="/"
		extends="struts-default,json-default">
		<action name="order_*" class="OrderAction" method="{1}">
			<result name="saveSuccess">/front/order.jsp</result>
			<result name="login">/front/login.jsp</result>
			<result name="findByOidSuccess">/front/order.jsp</result>
			<result name="findByUidSuccess">/front/orderList.jsp</result>
			<result name="payOrder">/front/payok.jsp</result>
		</action>
	</package>
</struts>
